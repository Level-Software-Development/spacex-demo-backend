{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "serverless offline start --stage local --noPrependStageInUrl",
    "test": "node -r dotenv/config node_modules/.bin/jest --runInBand --testTimeout=30000",
    "populate-app": "node_modules/.bin/ts-node helpers/populate.ts",
    "prepare-app": "npm run populate-app",
    "lint:tsc": "./node_modules/.bin/tsc --noEmit -p tsconfig.json",
    "lint:tslint": "./node_modules/.bin/tslint -c tslint.production.json ./**/*.ts",
    "lint:types": "node_modules/.bin/ts-node server.ts",
    "lint": "run-p lint:tslint lint:tsc lint:types",
    "ci": "run-p lint test",
    "dev": "node --inspect ./node_modules/.bin/serverless offline --port 3000",
    "delete:types": "rm -rf ../common/types/backend/*.ts",
    "generate:types": "npm run delete:types && npm run generate:codegen",
    "generate:codegen": "graphql-codegen --config codegen.yml",
    "deploy": "chmod +x server.ts && ./node_modules/.bin/serverless deploy"
  },
  "author": "app",
  "license": "ISC",
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true
  },
  "dependencies": {
    "@apollo/server": "^4.2.1",
    "@sentry/serverless": "^7.21.1",
    "@sentry/tracing": "^7.21.1",
    "@supercharge/promise-pool": "^2.3.2",
    "@vendia/serverless-express": "^4.10.1",
    "bluebird": "3.7.2",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^7.7.7",
    "faker": "^6.6.6",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.0.0",
    "luxon": "^3.1.0",
    "mysql2": "^2.3.3",
    "rate-limiter-flexible": "^2.4.1",
    "request-ip": "^3.3.0",
    "sequelize": "^6.25.8",
    "supertest": "^6.3.1",
    "umzug": "^3.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.12",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.8.2",
    "@graphql-codegen/typescript-resolvers": "^2.7.7",
    "@types/bluebird": "^3.5.37",
    "@types/express-jwt": "^7.4.2",
    "@types/express-session": "^1.17.5",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/luxon": "^3.1.0",
    "@types/node": "^18.11.9",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@types/umzug": "^2.3.3",
    "apollo": "^2.34.0",
    "aws-sdk": "^2.1261.0",
    "jest": "^29.3.1",
    "serverless": "^3.25.0",
    "serverless-offline": "^11.5.0",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  }
}
