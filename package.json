{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "serverless offline start --stage local --noPrependStageInUrl",
    "test": "node -r dotenv/config node_modules/.bin/jest --runInBand --testTimeout=30000",
    "populate-app": "node_modules/.bin/ts-node helpers/populate.ts",
    "prepare-app": "npm run populate-app",
    "lint:tsc": "./node_modules/.bin/tsc --noEmit -p tsconfig.json",
    "lint:tslint": "./node_modules/.bin/tslint -c tslint.production.json ./**/*.ts",
    "lint:types": "node_modules/.bin/ts-node server.ts",
    "lint": "run-p lint:tslint lint:tsc lint:types",
    "ci": "run-p lint test",
    "dev": "node --inspect ./node_modules/.bin/serverless offline --port 3000",
    "delete:types": "rm -rf ../common/types/backend/*.ts",
    "generate:types": "npm run delete:types && npm run generate:codegen",
    "generate:codegen": "graphql-codegen --config codegen.yml",
    "deploy": "chmod +x server.ts && ./node_modules/.bin/serverless deploy"
  },
  "author": "app",
  "license": "ISC",
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true
  },
  "dependencies": {
    "@sentry/serverless": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "@supercharge/promise-pool": "^2.0.0",
    "apollo-server-express": "^2.22.2",
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.27.2",
    "bluebird": "3.7.2",
    "body-parser": "1.20.0",
    "cross-fetch": "^3.1.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^7.7.7",
    "faker": "^5.4.0",
    "graphql": "^14.7.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.0.0",
    "luxon": "^2.0.2",
    "mysql2": "^2.2.5",
    "rate-limiter-flexible": "^2.1.15",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "sequelize": "^4.44.4",
    "supertest": "^4.0.2",
    "umzug": "^2.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@types/bluebird": "^3.5.33",
    "@types/express-jwt": "^7.4.2",
    "@types/express-session": "^1.17.3",
    "@types/faker": "^5.1.7",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/luxon": "^2.0.9",
    "@types/node": "^13.13.45",
    "@types/node-fetch": "^2.5.8",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.2",
    "apollo": "^2.32.5",
    "aws-sdk": "^2.858.0",
    "jest": "^25.5.2",
    "npm-run-all": "^4.1.5",
    "serverless": "^3.1.0",
    "serverless-offline": "^8.4.0",
    "serverless-plugin-typescript": "^2.1.1",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.10.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.9"
  }
}
